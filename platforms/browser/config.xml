<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tr.eurobano" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EUROBANO</name>
    <description>
        EUROBANO Mobile App
    </description>
    <author email="hi@miova.com.tr" href="https://miova.com.tr">
       Miova 360 Full Service Digital Agency
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
